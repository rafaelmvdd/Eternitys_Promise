meters":["Wolter:"]},{"code":401,"indent":0,"parameters":["Funny how the prey thinks the predator is"]},{"code":401,"indent":0,"parameters":["pathet-"]},{"code":101,"indent":0,"parameters":["Party",1,0,2]},{"code":401,"indent":0,"parameters":["Markus:"]},{"code":401,"indent":0,"parameters":["Is that protesting I hear? As a weakling,"]},{"code":401,"indent":0,"parameters":["you hardly have any right to do so!"]},{"code":101,"indent":0,"parameters":["Party",5,0,2]},{"code":401,"indent":0,"parameters":["Wolter:"]},{"code":401,"indent":0,"parameters":["Indignant swine! I'll show you who is the "]},{"code":401,"indent":0,"parameters":["weakling here!"]},{"code":101,"indent":0,"parameters":["Party",3,0,2]},{"code":401,"indent":0,"parameters":["Rook:"]},{"code":401,"indent":0,"parameters":["He's got their attention on us! Let's hurry"]},{"code":401,"indent":0,"parameters":["and distract them so the others can escape!"]},{"code":301,"indent":0,"parameters":[0,2,false,false]},{"code":221,"indent":0,"parameters":[]},{"code":201,"indent":0,"parameters":[0,10,12,10,0,0]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":0,"stepAnime":false,"through":false,"trigger":4,"walkAnime":true}],"x":0,"y":0},
